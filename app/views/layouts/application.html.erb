<!DOCTYPE html>
<html>
  <head>
    <title><%= @title %></title>
    <%= render 'layouts/stylesheets' %>
    <%= javascript_include_tag  'jquery-1.4.2.min.js', 'jquery-ui-1.8.5.custom.min.js', 
                                'autocomplete-rails.js', 'rails.js' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="container">
      <%= render 'layouts/header' %>
      <div id="content">
        <%= yield %>
      </div>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
